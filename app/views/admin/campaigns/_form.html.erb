<%= form_for [:admin, @campaign.organization, @campaign] do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :deadline %><br>
    <%= f.text_field :deadline %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', admin_organization_campaigns_url(@campaign.organization)%>