<h2 class="featurette-heading">Add an Organization</h2>
<%= form_for :organization, url: admin_organizations_path do |f| %>
    <p>
      <%= f.label :name %><br>
      <%= f.text_field :name %>
    </p>

    <p>
      <%= f.label :contact_name %><br>
      <%= f.text_field :contact_name %>
    </p>

    <p>
      <%= f.label :contact_email %><br>
      <%= f.text_field :contact_email %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>